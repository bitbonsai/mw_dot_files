[core]
	editor = mate -w
	excludesfile = /Users/mw/.gitignore

[color]
    ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan

  [color]
    ui = true
  [color "diff"]
    whitespace = red reverse
  [core]
    whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol


[diff]
	renames = true

[alias]
	s = status
	d = diff
	ci = commit -v
	cia = commit -v -a
	co = checkout
	cp = cherry-pick
	p = push
	l = log
	ll = log -p
	lt = log trunk..
	llt = log -p trunk..
	lm = log master..
	llm = log -p master..
	b = branch
	who = shortlog -s --
	who-did-what = log --pretty=format:%an - %s

[user]
	name = mwolff
	email = m@bitbonsai.com
[github]
	user = mwolff
	token = 
[push]
	default = matching
